[clusterparameter]

beginMinStationDistance = 1
endMinStationDistance=59.9
beginMaxStationDistance=60
endMaxStationDistance=90

maxCluster = 8

minStationAroundInitialCluster = 5
initialstationdistance = 10
cutoff = 30
runs = 5


[unit in deegree]
#minimum distance of initial centroids
centroidminDistance = 20

comparedelta = 2

#endcheck
#maximum distance from station to cluster center
stationdistance = 10
minClusterStation = 10


[traveltime calculation options]

tttopt = -ph P

[algorithm method]
#1 zoning
#2 kmeans
cam = 2

[general parameter]

depths=0,0,5
filters=2
dimx = 10
dimy = 10
minDist = 30
maxDist = 95
step = 5
winlen = 7
forerun = 200
duration = 500
security = 60
gridspacing = 0.02
ttphase=P
export_unfiltered=false
export_filtered=false
export_resampled=false
new_frequence = 10



[process parameter]

ncore = 2

[focal mechanism solution values from event file]

fm = 1

[xcorrskript parameter]

xcorr=1
autoxcorrcorrectur = 1
xcorrtreshold = 0.6

#filter for referencestation for automatic picker
refstationfreqmin=1
refstationfreqmax=20
refstationcorners=4
refstationzph=false
refsta=0.5
reflta=4

plotcopy=plot1,plot2,plot3,plot4,rerun,plot5,plot6,plot7,plot8

plot1 = semblance-plot.sh
plot2 = summary-plot.sh
plot3 = plot-cluster.sh
rerun = rerun_sum.py
plot4 = platebound.gmt
plot5 = semblance-plot_multiple_depths.sh
plot6 = summary-plot-point_multiple_depths.sh
plot7 = plot.py
plot8 = rerun_multiple_depths_sum.py

